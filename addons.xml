<addon id="plugin.video.metallic" name="Metallic" version="2.1" provider-name="vitorp07, OpenELEQ, MetaTeam">
	<requires>
		<import addon="xbmc.python" version="2.19.0"/>
		<import addon="script.module.requests" version="2.9.1"/>
		<import addon="script.module.futures" version="2.2.0"/>
	</requires>
	<extension point="xbmc.python.pluginsource" library="default.py">
		<provides>executable</provides>
	</extension>
	<extension point="xbmc.service" library="service.py" start="[login|startup]">
	</extension>
	<extension point="kodi.context.item">
		<menu id="kodi.core.main">
			<menu>
				<label>Metallic</label>
				<item library="context_toggle.py">
					<label>Preferred toggle</label>
					<visible>!System.HasAddon(context.metallic) + !Skin.HasSetting(Toggling)</visible>
				</item>
				<item library="context_movie.py">
					<label>$ADDON[plugin.video.metallic 30602]</label>
					<visible>StringCompare(ListItem.dbtype,movie) + SubString(ListItem.Path, plugin.video.metallic)</visible>
				</item>
				<item library="context_tvshow.py">
					<label>$ADDON[plugin.video.metallic 30602]</label>
					<visible>StringCompare(ListItem.dbtype,tvshow) + SubString(ListItem.Path, plugin.video.metallic)</visible>
				</item>
				<item library="context_live.py">
					<label>$ADDON[plugin.video.metallic 30602]</label>
					<visible>[SubString(ListItem.FileNameAndPath, plugin.video.metallic\\Live) | SubString(ListItem.FileNameAndPath, plugin.video.metallic\\Live)]</visible>
				</item>
				<item library="context_playable.py">
					<label>$ADDON[plugin.video.metallic 30622]</label>
					<visible>!System.HasAddon(context.metallic) + !Skin.HasSetting(Contexting)</visible>
				</item>
			</menu>
		</menu>
	</extension>
	<extension point="xbmc.addon.metadata">
		<platform>all</platform>
		<summary lang="en">TMDb, TVDb, Trakt, AudioDb and LastFM browser</summary>
		<description lang="en">Browse The MovieDb, The TVDb, Trakt, The AudioDb and LastFM and pass info along to library or an addon. Needs player-files and the appropriate addon to play the files.</description>
		<supportedcontent>
			<content>movies</content>
			<content>tvshows</content>
			<content>episodes</content>
			<content>musicvideos</content>
			<content>music</content>
			<content>livetv</content>
		</supportedcontent>
	</extension>
</addon>
<addon id="script.extendedinfo" name="ExtendedInfo Mod" version="6.0.7" provider-name="vitorp07, OpenELEQ, phil65">
	<requires>
		<import addon="xbmc.python" version="2.19.0"/>
		<import addon="plugin.video.metallic" version="1.3"/>
		<import addon="plugin.video.youtube" version="5.3.10"/>
		<import addon="script.module.pil" version="1.1.7"/>
		<import addon="script.module.requests" version="1.1.0"/>
		<import addon="script.module.simplejson" version="2.0.10"/>
		<import addon="script.module.urlresolver" version="1.0.0"/>
		<import addon="script.module.youtube.dl" version="14.519.1"/>
	</requires>
	<extension point="xbmc.python.script" library="default.py">
		<provides>executable</provides>
	</extension>
	<extension point="xbmc.python.library" library="default.py" />
	<extension point="xbmc.python.pluginsource" library="plugin.py">
		<provides>video</provides>
	</extension>
	<extension point="kodi.context.item">
		<menu id="kodi.core.main">
			<item library="context.py">
				<label>32999</label>
				<visible>String.IsEqual(ListItem.dbtype,movie) | String.IsEqual(ListItem.dbtype,tvshow) | String.IsEqual(ListItem.dbtype,season) | String.IsEqual(ListItem.dbtype,actor) | String.IsEqual(ListItem.dbtype,director)</visible>
			</item>
		</menu>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">ExtendedInfo Mod</summary>
		<description lang="en">This script offers various possibilities to display online content inside skins as well as a complete MovieDB browser including info dialogs for various media types. Used services are themoviedb.org, theaudiodb.org, Trakt.TV, omdbapi.com and a lot more. Needs skin implementation for lot of functions. Uses Metallic for library functions.</description>
		<platform>all</platform>
	</extension>
</addon>
